[Unit]
Description=Portal calendar puppeteer server
After=network-online.target

[Service]
WorkingDirectory=/<PATH>/portal_calendar
Environment="PORT=8083"
ExecStart="node puppeteer_server.js "
Type=simple
Restart=always
TimeoutStartSec=infinity
TimeoutStopSec=600

User=<USERNAME>
Group=<GROUPNAME>

[Install]
WantedBy=multi-user.target