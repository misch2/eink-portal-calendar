{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning",
            "Microsoft.EntityFrameworkCore.Database": "Warning"
        }
    },
    "ConnectionStrings": {
        "DefaultConnection": "Data Source={DataPath}/database/calendar.db"
    },
    "Paths": {
        "GeneratedImages": "{DataPath}/generated_images"
    },
    "AllowedHosts": "*",
    "URLs": {
        "BaseURL": "http://localhost:5001/"
    },
    "Kestrel": {
        "Endpoints": {
            "Http": {
                "Url": "http://127.0.0.1:5001"
            }
        }
    },
    "UI": {
        "EnvironmentCssFile": "css/environment/staging.css"
    }
}
