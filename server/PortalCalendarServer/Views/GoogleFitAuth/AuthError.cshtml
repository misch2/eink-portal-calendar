@using PortalCalendarServer.Controllers
@{
    Layout = "_Layout";
    ViewData["Title"] = "Authentication Error";
}

<h1>@ViewData["Title"]</h1>

<div class="alert alert-danger">
    <h4>Authentication Failed</h4>
    <p>@ViewData["Error"]</p>
</div>

<p>
    <a href="@Url.Action(nameof(UiController.SelectDisplay), "Ui")" class="btn btn-primary">Return to Home</a>
</p>
